<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.chenxin.shop.modules.dao.TestDao">
    <resultMap id="users" type="com.chenxin.shop.modules.demo.entity.Users">
        <id property="id" column="id"/>
        <result property="orderId" column="order_id"/>
        <result property="name" column="name"/>
        <result property="address" column="address"/>
    </resultMap>

    <select id="getUserList" resultMap="users">
        select id, name, order_id, address from users
    </select>

    <!--  xml方式用page分页  -->
    <select id="getXmlUserPageList" resultType="com.chenxin.shop.modules.demo.entity.Users">
        select id, name, order_id, address from users
    </select>
</mapper>
